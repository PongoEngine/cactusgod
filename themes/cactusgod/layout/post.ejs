<%- partial('_partial/article', {item: page, index: false}) %>
<ul class="pager">
  <% if (page.prev){ %>
      <li class="pager-left"><a href="<%- page.prev.permalink %>">↩ <%- page.prev.title %></a></li>
  <% } %>
  <% if (page.next){ %>
      <li class="pager-right"><a href="<%- page.next.permalink %>"><%- page.next.title %> ↪</a></li>
  <% } %>
</ul>